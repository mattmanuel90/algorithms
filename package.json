{
  "name": "algorithms",
  "version": "1.0.0",
  "description": "A collection of algorithms built with typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "tsc && jest --no-cache"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "jest": "^21.2.1",
    "typescript": "^2.5.3"
  },
  "devDependencies": {
    "@types/jest": "^21.1.2",
    "ts-jest": "^21.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts"
    ]
  }
}
